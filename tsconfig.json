{
  "compilerOptions": {
      "target": "es2017",
      "module": "commonjs",
      "removeComments": false,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true,
      "noImplicitAny": false,
      "resolveJsonModule": true,
      "baseUrl": ".",
      "paths": {},
      "outDir": "./dist",
      "moduleResolution": "node",
      "types": [
          "node"
      ],
      "typeRoots": [
          "./src/interface/index.d.ts"
      ]
  },
  "filesGlob": [
      "./src/**/*.ts",
      "./src/**/*.js"
  ],
  "include": [
      "./src/**/*.ts"
  ],
  "exclude": [
      "node_modules"
  ]
}